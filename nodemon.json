{
    "restartable": "rs",
    "ignore": [
        "**/*.test.ts",
        "**/*.spec.ts",
        "dist/",
        "coverage/",
        ".git",
        "node_modules"
    ],
    "watch": ["src/"],
    "execMap": {
      "ts": "node -r ts-node/register"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
  }